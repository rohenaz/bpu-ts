{
  "name": "bpu-ts",
  "version": "0.0.2",
  "description": "Bitcoin Processing Unit - Typescript",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/src/index.js",
  "module": "./esm/src/index.js",
  "type": "module",
  "author": "wildsatchmo",
  "contributors": [
    "wildsatchmo",
    "_unwriter"
  ],
  "files": [
    "dist",
    "src",
    "esm"
  ],
  "engines": {
    "node": ">= 14"
  },
  "license": "ISC",
  "scripts": {
    "check": "tsc --noEmit",
    "clean": "yarn cache clean && rimraf dist/ && rimraf esm/ parcel-bundle-reports .parcel-cache",
    "lint": "eslint ./src",
    "prebuild": "npm run clean",
    "build": "rimraf  dist/ && tsc --module commonjs -outDir dist && tsc --module esnext -outDir esm",
    "parcelbuild": "rimraf parcel-bundle-reports dist/ && parcel build . --no-cache --target types --target module --target legacy --reporter @parcel/reporter-bundle-analyzer",
    "test": "jest --collectCoverage"
  },
  "dependencies": {
    "@ts-bitcoin/core": "^2.2.0",
    "bitcoind-rpc": "^0.9.1",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "@jest/globals": "^29.3.1",
    "@jest/types": "^29.3.1",
    "@types/bitcoind-rpc": "github:rohenaz/bitcoind-rpc-types",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-config": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tsc": "^2.0.4",
    "typescript": "^4.9.4"
  }
}