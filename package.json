{
  "name": "bpu-ts",
  "version": "0.0.2",
  "description": "Bitcoin Processing Unit - Typescript",
  "types": "./dist/bpu.d.ts",
  "main": "dist/bpu.js",
  "module": "dist/bpu.module.js",
  "legacy": "dist/bpu.cjs",
  "type": "module",
  "author": "wildsatchmo",
  "contributors": [
    "wildsatchmo",
    "_unwriter"
  ],
  "targets": {
    "main": {
      "isLibrary": true,
      "context": "node",
      "source": "src/index.ts",
      "optimize": false,
      "engines": {
        "node": ">=14"
      }
    },
    "module": {
      "isLibrary": true,
      "context": "node",
      "optimize": false,
      "source": "src/index.ts"
    },
    "types": {
      "source": "src/index.ts"
    },
    "legacy": {
      "includeNodeModules": true,
      "distDir": "dist",
      "source": "src/browser.ts",
      "optimize": true,
      "scopeHoist": true,
      "context": "browser",
      "outputFormat": "commonjs",
      "engines": {
        "browsers": "Chrome 80"
      }
    }
  },
  "engines": {
    "node": ">= 14"
  },
  "license": "ISC",
  "scripts": {
    "check": "tsc --noEmit",
    "clean": "yarn cache clean && rimraf dist/ parcel-bundle-reports .parcel-cache",
    "lint": "eslint ./src",
    "build": "rimraf parcel-bundle-reports dist/ && parcel build . --target types --target module --target main --target legacy --reporter @parcel/reporter-bundle-analyzer",
    "test": "jest --collectCoverage"
  },
  "dependencies": {
    "@ts-bitcoin/core": "^2.2.0",
    "bitcoind-rpc": "^0.9.1",
    "dotenv": "^16.0.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@jest/types": "^29.3.1",
    "@parcel/config-default": "^2.8.2",
    "@parcel/core": "^2.8.2",
    "@parcel/reporter-bundle-analyzer": "^2.8.2",
    "@parcel/transformer-typescript-tsc": "^2.8.2",
    "@parcel/transformer-typescript-types": "^2.8.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@types/bitcoind-rpc": "github:rohenaz/bitcoind-rpc-types",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "https-browserify": "^1.0.0",
    "jest": "^29.3.1",
    "jest-config": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mocha": "^10.2.0",
    "prettier": "^2.8.1",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "rimraf": "^3.0.2",
    "stream-http": "^3.1.0",
    "ts-jest": "^29.0.3",
    "tsc": "^2.0.4",
    "typescript": "^4.9.4",
    "url": "^0.11.0"
  }
}